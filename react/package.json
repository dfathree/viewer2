{
  "name": "viewer2",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "linebreak-style": [
        "error"
      ],
      "no-case-declarations": [
        "error"
      ],
      "no-extra-boolean-cast": [
        "error"
      ],
      "no-extra-semi": [
        "error"
      ],
      "no-tabs": [
        "error"
      ],
      "no-undef": [
        "error"
      ],
      "no-unused-vars": [
        "error"
      ],
      "semi": [
        "error"
      ]
    }
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
