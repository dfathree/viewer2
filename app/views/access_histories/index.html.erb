<p id="notice"><%= notice %></p>

<h1>Access Histories</h1>

<table>
  <thead>
    <tr>
      <th>Board</th>
      <th>Thre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @access_histories.each do |access_history| %>
      <tr>
        <td><%= access_history.board_id %></td>
        <td><%= access_history.thre_id %></td>
        <td><%= link_to 'Show', access_history %></td>
        <td><%= link_to 'Edit', edit_access_history_path(access_history) %></td>
        <td><%= link_to 'Destroy', access_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Access History', new_access_history_path %>
